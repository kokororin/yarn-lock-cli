{
  "name": "yarn-lock-cli",
  "version": "0.1.1",
  "description": "CLI tool for transforming registry in yarn.lock",
  "bin": {
    "yarn-lock": "bin/yarn-lock.js"
  },
  "scripts": {
    "start": "npm run watch",
    "build": "npm run trash && tsc -p ./",
    "trash": "trash lib",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kokororin/yarn-lock-cli.git"
  },
  "keywords": [
    "yarn",
    "cli"
  ],
  "author": "kokororin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kokororin/yarn-lock-cli/issues"
  },
  "homepage": "https://github.com/kokororin/yarn-lock-cli#readme",
  "devDependencies": {
    "@types/inquirer": "^8.0.0",
    "@types/node": "^20.8.8",
    "trash-cli": "^5.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^11.1.0",
    "inquirer": "^8.0.0",
    "pjson": "^1.0.9"
  },
  "files": [
    "lib",
    "bin"
  ]
